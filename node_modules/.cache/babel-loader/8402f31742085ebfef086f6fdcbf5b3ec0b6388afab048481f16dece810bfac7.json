{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { APIService } from '../http/APIService';\nconst apiService = new APIService();\nexport default {\n  name: 'StockCreate',\n  data() {\n    return {\n      stock: {},\n      pageTitle: \"Add New Stock\",\n      isUpdate: false\n    };\n  },\n  methods: {\n    createStock() {\n      apiService.addNewStock(this.stock).then(response => {\n        if (response.status === 201) {\n          this.stock = response.data;\n          this.$router.push('/stock-list');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    updateStock() {\n      apiService.updateStock(this.stock).then(response => {\n        if (response.status === 200) {\n          this.stock = response.data;\n          this.$router.push('/stock-list');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    cancelOperation() {\n      this.$router.push('/stock-list');\n    }\n  },\n  mounted() {\n    if (this.$route.params.pk) {\n      this.pageTitle = \"Edit Stock\";\n      this.isUpdate = true;\n      apiService.getStock(this.$route.params.pk).then(response => {\n        this.stock = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["APIService","apiService","name","data","stock","pageTitle","isUpdate","methods","createStock","addNewStock","then","response","status","$router","push","catch","error","console","updateStock","cancelOperation","mounted","$route","params","pk","getStock"],"sources":["/Users/tomatosauce/Desktop/assign3-investments-part2-frontend-jaketamayo/investments-fe/src/components/StockCreate.vue"],"sourcesContent":["<template>\n    <div class=\"container-fluid\">\n        <div class=\"row align-items-center justify-content-center\">\n            <div class=\"col-12 col-sm-10 col-md-10 col-lg-6\">\n                <div class=\"card\">\n                    <div class=\"card-header\">{{ pageTitle }}</div>\n                    <div class=\"card-body\">\n                        <form ref=\"form\">\n                            <div class=\"form-group row py-2\">\n                                <label class=\"col-4\">Stock Name</label>\n                                <div class=\"col-8\">\n                                    <input v-model=\"stock.name\" type=\"text\" class=\"form-control\">\n                                </div>\n                            </div>\n                            <div class=\"form-group row py-2\">\n                                <label class=\"col-4\">Stock Value</label>\n                                <div class=\"col-8\">\n                                    <input v-model=\"stock.value\" type=\"number\" class=\"form-control\">\n                                </div>\n                            </div>\n                            <div class=\"form-group row py-2\">\n                                <label class=\"col-4\">Purchase Date</label>\n                                <div class=\"col-8\">\n                                    <input v-model=\"stock.purchase_date\" type=\"date\" class=\"form-control\">\n                                </div>\n                            </div>\n                            <div class=\"row justify-content-around\">\n                                <button v-if=\"!isUpdate\" type=\"button\" class=\"btn btn-primary col-4\"\n                                    @click=\"createStock\">Save</button>\n                                <button v-if=\"isUpdate\" type=\"button\" class=\"btn btn-primary col-4\"\n                                    @click=\"updateStock\">Update</button>\n                                <button type=\"button\" class=\"btn btn-secondary col-4\"\n                                    @click=\"cancelOperation\">Cancel</button>\n                            </div>\n                        </form>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { APIService } from '../http/APIService';\nconst apiService = new APIService();\n\nexport default {\n    name: 'StockCreate',\n    data() {\n        return {\n            stock: {},\n            pageTitle: \"Add New Stock\",\n            isUpdate: false,\n        };\n    },\n    methods: {\n        createStock() {\n            apiService.addNewStock(this.stock).then(response => {\n                if (response.status === 201) {\n                    this.stock = response.data;\n                    this.$router.push('/stock-list');\n                }\n            }).catch(error => {\n                console.error(error);\n            });\n        },\n        updateStock() {\n            apiService.updateStock(this.stock).then(response => {\n                if (response.status === 200) {\n                    this.stock = response.data;\n                    this.$router.push('/stock-list');\n                }\n            }).catch(error => {\n                console.error(error);\n            });\n        },\n        cancelOperation() {\n            this.$router.push('/stock-list');\n        },\n    },\n    mounted() {\n        if (this.$route.params.pk) {\n            this.pageTitle = \"Edit Stock\";\n            this.isUpdate = true;\n            apiService.getStock(this.$route.params.pk).then(response => {\n                this.stock = response.data;\n            }).catch(error => {\n                console.error(error);\n            });\n        }\n    }\n};\n</script>\n"],"mappings":";AA2CA,SAASA,UAAS,QAAS,oBAAoB;AAC/C,MAAMC,UAAS,GAAI,IAAID,UAAU,CAAC,CAAC;AAEnC,eAAe;EACXE,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,CAAC,CAAC;MACTC,SAAS,EAAE,eAAe;MAC1BC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAG;MACVP,UAAU,CAACQ,WAAW,CAAC,IAAI,CAACL,KAAK,CAAC,CAACM,IAAI,CAACC,QAAO,IAAK;QAChD,IAAIA,QAAQ,CAACC,MAAK,KAAM,GAAG,EAAE;UACzB,IAAI,CAACR,KAAI,GAAIO,QAAQ,CA